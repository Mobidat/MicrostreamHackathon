<!DOCTYPE xml>
<?xdevuiml version="1.0"?>
<?template NONE?>
<?import com.vaadin.flow.component.html.H1?>
<?import com.vaadin.flow.component.html.H2?>
<?import com.vaadin.flow.component.orderedlayout.VerticalLayout?>
<?import com.zillus.coronadiary.ui.MainLayout?>
<VerticalLayout xmlns:x="http://xml.xdev-software.com/xdevuiml" x:routeLayout="MainLayout" spacing="false" x:name="HomeView" x:route="">
    <x:constraints zpos="0" width="-1px" height="-1px" />
    <H1 x:name="headLine" text="Corona Diary">
        <x:constraints width="0px" alignment="CENTER" height="0px" flex-grow="0.0" />
        <x:styles>
            <entry name="font-family" value="times" />
            <entry name="color" value="Orange" />
        </x:styles>
    </H1>
    <H2 x:name="h22" text="by Frank Zillus 2021">
        <x:constraints width="0px" height="0px" flex-grow="0.0" />
    </H2>
    <H2 x:name="h2Items" text="none">
        <x:constraints width="0px" height="0px" flex-grow="0.0" />
    </H2>
    <H2 x:name="h2Patient" text="none">
        <x:constraints width="0px" height="0px" flex-grow="0.0" />
    </H2>
</VerticalLayout>
